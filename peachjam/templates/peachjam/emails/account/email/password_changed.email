{% extends 'peachjam/emails/layouts/base.html' %}
{% load i18n %}
{% block subject %}{% blocktrans trimmed %}Your password has been changed{% endblocktrans %}{% endblock %}
{% block body %}
  <p>{% blocktrans trimmed %}This is a confirmation that your password has been changed.{% endblocktrans %}</p>
  <p>{% blocktrans trimmed %}If you didn't make this change, please reset your password immediately and contact support.{% endblocktrans %}</p>
  <p>{% blocktrans trimmed with app_name=APP_NAME %}The {{ app_name }} Team{% endblocktrans %}</p>
{% endblock %}
