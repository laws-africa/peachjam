{% extends 'peachjam/emails/layouts/base.html' %}
{% load i18n %}
{% block subject %}{% blocktrans trimmed %}An email address was removed from your account{% endblocktrans %}{% endblock %}
{% block body %}
  <p>{% blocktrans trimmed %}Email address {{ deleted_email }} has been removed from your account.{% endblocktrans %}</p>
  <p>{% blocktrans trimmed %}If you didn't request this change, please secure your account immediately.{% endblocktrans %}</p>
  <p>{% blocktrans trimmed %}The {{ APP_NAME }} Team{% endblocktrans %}</p>
{% endblock %}
