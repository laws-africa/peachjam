{% extends 'peachjam/emails/layouts/base.html' %}
{% load i18n %}
{% block subject %}{% blocktrans trimmed %}Your password has been reset{% endblocktrans %}{% endblock %}
{% block body %}
  <p>{% blocktrans trimmed %}Your password has been reset successfully.{% endblocktrans %}</p>
  <p>{% blocktrans trimmed %}If you didn't perform this action, please reset your password immediately and contact support.{% endblocktrans %}</p>
  <p>{% blocktrans trimmed %}The {{ APP_NAME }} Team{% endblocktrans %}</p>
{% endblock %}
